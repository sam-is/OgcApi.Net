            "Schema": "dbo",
            "Table": "LineStrings",
            "GeometryColumn": "Geom",
            "GeometryDataType": "geometry",
            "GeometryGeoJsonType": "LineString",
            "GeometrySrid": 3857,
            "IdentifierColumn": "Id",
            "Properties": [
              "Name"
            ],
            "AllowCreate": true,
            "AllowReplace": true,
            "AllowUpdate": true,
            "AllowDelete": true
          }
        }
      },
